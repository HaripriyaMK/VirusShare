            "<h1>Welcome to Jerusalem</h1> ",
            "<div style=\"margin: -30px 0 0 20px;\">",
            "<script type=\"text/javascript\"><!--",
            "google_ad_client = \"pub-7160301037253755\";",
            "/* 468x60, created 6/28/11 */",
            "google_ad_slot = \"7269738587\";",
