            "<HR>",
            "//\u0221\u01f0\u0434\u0434\u0138\u013f( Memo \u02be)",
            "<a target=\"_blank\" href=\"http://www.xuedelphi.cn\" class=\"wordstyle\">procedure</a> T<a target=\"_blank\" href=\"http://www.xuedelphi.cn\" class=\"wordstyle\">Form</a>1.<a target=\"_blank\" href=\"http://www.xuedelphi.cn\" class=\"wordstyle\">Form</a>Create(Sender: TObject);",
            "  arr: array[0..25] of Integer;",
            "  i: Integer;",
            "<a target=\"_blank\" href=\"http://www.xuedelphi.cn\" class=\"wordstyle\">begin</a>",
            "  GetCharWidth(Canvas.Handle, Ord('A'), Ord('Z'), arr);",
            "  //GetCharWidth32(Canvas.Handle, Ord('A'), Ord('Z'), arr); &#123;\u04b2&#125;",
            "  Memo1.Clear;",
            "  for i := 0 to 25 do",
            "  <a target=\"_blank\" href=\"http://www.xuedelphi.cn\" class=\"wordstyle\">begin</a>",
            "    Memo1.Lines.Add(Char(Ord('A')+i) + #9 + IntToStr(arr[i]));",
            "  end;",
            "end;",
            "<HR>",
