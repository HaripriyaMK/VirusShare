            "# /etc/rc.d/rc.sysinit - run once at boot time",
            "# Taken in part from Miquel van Smoorenburg's bcheckrc.",
            "HOSTNAME=`/bin/hostname`",
            "HOSTTYPE=`uname -m`",
            "unamer=`uname -r`",
            "set -m",
            "if [ -f /etc/sysconfig/network ]; then",
            "    . /etc/sysconfig/network",
            "if [ -z \"$HOSTNAME\" -o \"$HOSTNAME\" = \"(none)\" ]; then",
            "    HOSTNAME=localhost",
            "if [ ! -e /proc/mounts ]; then ",
            "\tmount -n -t proc /proc /proc",
            "\tmount -n -t sysfs /sys /sys >/dev/null 2>&1",
            "if [ ! -d /proc/bus/usb ]; then",
            "\tmodprobe usbcore >/dev/null 2>&1 && mount -n -t usbfs /proc/bus/usb /proc/bus/usb",
            "else",
            "\tmount -n -t usbfs /proc/bus/usb /proc/bus/usb",
            ". /etc/init.d/functions",
            "# Check SELinux status",
            "selinuxfs=\"$(fstab_decode_str `LC_ALL=C awk '/ selinuxfs / { print $2 }' /proc/mounts`)\"",
            "SELINUX_STATE=",
            "if [ -n \"$selinuxfs\" ] && [ \"`cat /proc/self/attr/current`\" != \"kernel\" ]; then",
            "\tif [ -r \"$selinuxfs/enforce\" ] ; then",
            "\t\tSELINUX_STATE=`cat \"$selinuxfs/enforce\"`",
            "\telse",
            "\t\t# assume enforcing if you can't read it",
            "\t\tSELINUX_STATE=1",
            "if [ -n \"$SELINUX_STATE\" -a -x /sbin/restorecon ] && LC_ALL=C fgrep -q \" /dev \" /proc/mounts ; then",
            "\t/sbin/restorecon  -R /dev 2>/dev/null",
            "disable_selinux() {",
            "\techo $\"*** Warning -- SELinux is active\"",
            "\techo $\"*** Disabling security enforcement for system recovery.\"",
            "\techo $\"*** Run 'setenforce 1' to reenable.\"",
            "\techo \"0\" > \"$selinuxfs/enforce\"",
            "relabel_selinux() {",
            "    if [ -x /usr/bin/rhgb-client ] && /usr/bin/rhgb-client --ping ; then",
            "\tchvt 1",
            "    fi",
            "    # if /sbin/init is not labeled correctly this process is running in the ",
            "    # wrong context, so a reboot will be reuired after relabel",
            "    REBOOTFLAG=`restorecon -v /sbin/init`",
            "    AUTORELABEL=",
            "    . /etc/selinux/config",
            "    if [ \"$AUTORELABEL\" = \"0\" ]; then",
            " \trm -f  /.autorelabel ",
            " \techo",
            "\techo $\"*** Warning -- SELinux ${SELINUXTYPE} policy relabel is required. \"",
            " \techo $\"*** /etc/selinux/config indicates you want to manually fix labeling\"",
            " \techo $\"*** problems. Dropping you to a shell; the system will reboot\"",
            " \techo $\"*** when you leave the shell.\"",
            " \techo \"0\" > $selinuxfs/enforce",
            " \tsulogin",
            " \techo $\"Unmounting file systems\"",
            " \tumount -a",
            " \tmount -n -o remount,ro /",
            " \techo $\"Automatic reboot in progress.\"",
            " \treboot -f",
            "    else",
            " \techo",
            " \techo $\"*** Warning -- SELinux ${SELINUXTYPE} policy relabel is required.\"",
            " \techo $\"*** Relabeling could take a very long time, depending on file\"",
            " \techo $\"*** system size and speed of hard drives.\"",
            " \techo \"0\" > $selinuxfs/enforce",
            " \t/sbin/fixfiles restore > /dev/null 2>&1 ",
            " \trm -f  /.autorelabel ",
            " \tif [ ! -z \"$REBOOTFLAG\" ]; then ",
            " \t    echo $\"Automatic reboot in progress.\"",
            " \t    reboot -f",
            " \tfi",
            " \techo $SELINUX_STATE > $selinuxfs/enforce",
            " \tif [ -x /usr/bin/rhgb-client ] && /usr/bin/rhgb-client --ping ; then",
            " \t    chvt 8",
            " \tfi",
            "    fi",
            "key_is_random() {",
            "    [ \"$1\" = \"/dev/urandom\" -o \"$1\" = \"/dev/hw_random\" \\",
            "\t-o \"$1\" = \"/dev/random\" ]",
            "# Because of a chicken/egg problem, init_crypto must be run twice.  /var may be",
            "# encrypted but /var/lib/random-seed is needed to initialize swap.",
            "init_crypto() {",
            "    local have_random dst src key opt mode owner params makeswap skip arg opt",
            "    local param value rc ret mke2fs mdir",
            "    ret=0",
            "    have_random=$1",
            "    while read dst src key opt; do",
            "\t[ -z \"$dst\" -o \"${dst#\\#}\" != \"$dst\" ] && continue",
            "        [ -b \"/dev/mapper/$dst\" ] && continue;",
            "\tif [ \"$have_random\" = 0 ] && key_is_random \"$key\"; then",
            "\t    continue",
            "\tif [ -n \"$key\" -a \"x$key\" != \"xnone\" ]; then",
            "\t    if test -e \"$key\" ; then",
            "\t\towner=$(ls -l $key <br>bool(false)",
