            "   write(\"goto \"+key+\"v\");",
            " write(\":\"+key+\"i\");",
            " var v2=\"\";",
            " var cripple2=cripple && usedos6 && !echoblank &&",
            "          type==1 && s_seeks==\"\" && s_infects==\"\";",
            " if (s_seeks==\"\") v2=key+\"e\";",
