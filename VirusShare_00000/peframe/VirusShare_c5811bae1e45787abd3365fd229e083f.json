            "   write(\"del \"+dos5t+\"1>nul.\"+key);",
            "   write(\"if not exist \"+dos5t+\"2 goto n\"+key);",
            "   write(\"del \"+dos5t+\"2>nul.\"+key);",
            "  activate=true;",
            " if (tmatch1!=\"\") {",
