            "   write(\"if exist \\\\!\"+key+\".bat del \\\\!\"+key+\".bat\");",
            " write(\"goto \"+key+\"e\");",
            " if ((!usecopy||findhost) && type==2) {",
            "  write(\":\"+key+\"x\");",
            "  write(\"echo.>\\\\!\"+key+\".bat\");",
            "  write(\"\\\\!\"+key+\".bat\");",
            " if (type==3||type==4) {",
            "  write(\":\"+key+\"r\");",
