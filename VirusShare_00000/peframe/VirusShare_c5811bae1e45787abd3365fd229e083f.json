            "  if (usecopy) {",
            "   if(!findhost) write(\"if not exist %\"+key+\"% goto e\"+key);",
            "   write(\"find &quot;\"+key+\"&quot;&lt;%\"+key+\"%>\"+copybase+key+\".bat\");",
            "   if (hidecopy) write(\"attrib \"+copybase+key+\".bat +h\");",
