            "   write(\"if exist %2\\\\%\"+key+\".bat set \"+key+\"=%2\\\\%\"+key+\"%.bat\");",
            "   write(\"if exist %2%\"+key+\".bat set \"+key+\"=%2%\"+key+\"%.bat\");",
